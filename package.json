{
  "name": "react-redux-app",
  "version": "0.2.2",
  "description": "An opinionated development-ready React-Redux boilerplate that will instantly get you going.",
  "main": "index.html",
  "scripts": {
    "serve": "node server.js",
    "build": "./node_modules/webpack/bin/webpack.js --colors",
    "build-watch": "./node_modules/webpack/bin/webpack.js --colors --watch",
    "build-watch-progress": "./node_modules/webpack/bin/webpack.js --progress --colors --watch",
    "start": "concurrently --kill-others \"npm run build-watch\" \"npm run serve\"",
    "hot": "./node_modules/webpack-dev-server/bin/webpack-dev-server.js",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "repository": {
    "type": "git",
    "url": "git+https://miclaus@github.com/miclaus/create-react-redux-app.git"
  },
  "keywords": [
    "react",
    "redux",
    "react-router",
    "redux-devtools",
    "webpack",
    "hot",
    "es6",
    "babel",
    "sass",
    "flow"
  ],
  "author": "Iosif Miclaus <iosif@leinwand.cc> (https://github.com/miclaus)",
  "contributors": [
    "Valentina Pegler <valentina@leinwand.cc> (https://github.com/VLNTNA)"
  ],
  "license": "BSD-3-Clause",
  "bugs": {
    "url": "https://github.com/miclaus/create-react-redux-app/issues"
  },
  "homepage": "https://github.com/miclaus/create-react-redux-app#readme",
  "dependencies": {
    "axios": "^0.15.2",
